{
  "name": "@weft/server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "$HOME/.bun/bin/bun --watch src/index.ts",
    "build": "tsc",
    "start": "$HOME/.bun/bin/bun dist/index.js",
    "lint": "eslint .",
    "test": "vitest",
    "clean": "rm -rf dist .turbo node_modules",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "$HOME/.bun/bin/bun src/db/seed.ts"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-converter": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "@tensorflow/tfjs-node": "file:../tfjs-node-stub",
    "@vladmandic/face-api": "^1.7.15",
    "@weft/shared": "workspace:*",
    "axios": "^1.13.4",
    "better-auth": "^1.4.17",
    "canvas": "^3.2.1",
    "drizzle-orm": "^0.45.1",
    "form-data": "^4.0.5",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.8",
    "undici": "^7.19.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
